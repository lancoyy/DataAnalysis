function goTo(){  
  var page = $("#page").val();
  //获得总页数
	var index1 = htmlText.indexOf("共");
	var index2 = index1;
	do{
		index2+=1;
	}while(htmlText.charAt(index2+1)!="页");
	var tp = htmlText.substring(index1+1,index2+1);
  //判断输入是否合法
    if(parseInt(page) <= parseInt(tp) && parseInt(page)>=1 &&page.match(/^-?[0-9]+$/)!=null){
  	  
    	paging(page);
   }
   else{
  	 //超出页数范围则清空输入框
   	document.getElementById("page").value = "";
   }
}

